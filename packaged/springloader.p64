--[[pod_format="raw",created="2025-12-08 17:54:15",modified="2025-12-13 22:42:57",revision=12,xstickers={}]]

local Util = require("src/util")

local GuiCategoryView = require("src/gui/category_view")

local ViewTilesCommon = {}

-- Args:
--  x: number,
--  y: number,
--  width: number,
--  height: number,
--  big_icons: bool or "auto",
--  categories: table,
--  active_categories: table,
--  theme: table
function ViewTilesCommon.new(parent, args)
	local container = parent:attach {
		x = args.x,
		y = args.y,
		width = args.width,
		height = args.height
	}

	local category_x = 0
	local category_height = args.height
	
	if #args.active_categories >= 1 then
		local index = Util.index_by_key(args.categories, args.active_categories[1], "name")
		local active_category = args.categories[index]
		local use_big_icons
		
		if args.big_icons == "auto" then
			-- Need to populate now so we know how many items there are
			active_category:populate()
			
			use_big_icons = #active_category.items <= 15
		else
			use_big_icons = args.big_icons
		end
		
		if index > 0 then
			GuiCategoryView.new(container, {
				x = category_x,
				y = 0,
				width = args.width,
				height = category_height,
				grid_layout = true,
				big_icons = use_big_icons,
				category = active_category,
				theme = args.theme
			})
		end
	end
	
	return container
end

return ViewTilesCommon

