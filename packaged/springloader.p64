local Util = require("src/util")

local _save_dir = "/appdata/springloader"
local _save_path = _save_dir .. "/config.pod"

local _default_config = {
	_config_version = 1,
	app_folder = Util.is_web_player() and "/system" or "/desktop/apps",
	view = "auto_tiles",
	background = "bubbles",
	color_theme = "os",
	picotron_colors = false,
	color_blending = true,
	reduced_motion = false,
	sound = true,
	last_categories = {}
}

local Config = {
	current = {}
}

function Config.load()
	local fetched = fetch(_save_path)
	local modified = false
	
	if not fetched then
		fetched = _default_config
		modified = true
	else
		-- Add any new config keys so tinkerers can see them in podtree
		for k, v in pairs(_default_config) do
			if fetched[k] == nil then
				fetched[k] = v
				modified = true
			end
		end
	end
	
	if modified then Config.save(fetched) end
	
	return fetched
end

function Config.save(config)
	mkdir(_save_dir)
	store(_save_path, config)
end

function Config:load_current()
	self.current = Config.load()
end

function Config:save_current()
	Config.save(self.current)
end

return Config

