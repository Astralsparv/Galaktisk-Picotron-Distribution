local Class = require("src/class")
local Util = require("src/util")

local BGBubbles = Class.class {
	name = "Bubbles",
	config_name = "bubbles"
}

function BGBubbles.new()
	local bg = BGBubbles:new_obj({
		bubbles = {},
		timer = 0
	})

	for i = 1, 32 do
		local new_bubble = {
			x = rnd(480),
			y = rnd(270),
			x_velocity = rnd() * 16,
			y_velocity = -12 * (rnd() + 0.25),
			timer = rnd(10),
			radius = i
		}
		
		add(bg.bubbles, new_bubble)
	end
	
	return bg
end

function BGBubbles:update()
	self.timer += Util.time_delta
	
	for bubble in all(self.bubbles) do
		bubble.timer += Util.time_delta
		bubble.x += math.sin(bubble.timer * 0.5) * bubble.x_velocity * Util.time_delta
		bubble.y += bubble.y_velocity * Util.time_delta
		
		if bubble.x < -bubble.radius then
			bubble.x += 479 + bubble.radius * 2
		elseif bubble.x > 480 + bubble.radius then
			bubble.x -= 479 + bubble.radius * 2
		end
		
		if bubble.y < -bubble.radius then
			bubble.y = 270 + bubble.radius
		end
	end
end

function BGBubbles:draw(theme)
	cls(theme.back2)
	
	for bubble in all(self.bubbles) do
		circfill(bubble.x, bubble.y, bubble.radius, theme.back1)
	end
end

return BGBubbles

:: src/bg/picotron.lua
--[[pod_format="raw",created="2025-12-08 21:28:13",modified="2025-12-14 00:35:09",revision=37,xstickers={}]]

local Class = require("src/class")
local Util = require("src/util")

local BGPicotron = Class.class {
	name = "Picotron",
	config_name = "picotron",
	timer = 0
}

function BGPicotron.new()
	return BGPicotron:new_obj()
end

function BGPicotron:update()
	self.timer += Util.time_delta
	
	if self.timer >= 16 then
		self.timer -= 16
	end
end

function BGPicotron:draw(theme)
	cls(theme.back2)
	
	local sprite = get_spr(513)
	local max_scroll = math.min(sprite:width(), sprite:height())
	
	pal(7, theme.back1)
	camera(self.timer * 8 % max_scroll, self.timer * 8 % max_scroll)

	local tile_x = math.ceil(480 / sprite:width()) + 1
	local tile_y = math.ceil(270 / sprite:height()) + 1
	
	for y = 0, tile_y - 1 do
		for x = 0, tile_x - 1 do
			spr(513, x * sprite:width(), y * sprite:height())
		end
	end
	
	pal()
	camera()
end

return BGPicotron

