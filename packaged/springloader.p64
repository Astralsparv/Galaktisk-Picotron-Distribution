function _init()
	if env().sandbox then
		notify("Springloader isn't very useful while sandboxed! Exiting...")
		exit()
	end
	
	if Util.instance_running() then
		notify("Springloader is already running.")
		exit()
	end
	
	window {
		title = "Springloader",
		icon = --[[pod_type="gfx"]]unpod("b64:bHo0ABgAAAAWAAAA8AdweHUAQyAICAT3ACAXQAdwF3AHQBeg"),
		z = -1001, -- So we don't lose windowed apps that start in the current workspace
