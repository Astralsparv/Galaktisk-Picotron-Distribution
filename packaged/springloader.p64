--[[pod_format="raw",created="2025-12-07 00:25:36",modified="2025-12-12 22:22:05",revision=48,xstickers={}]]

local Util = require("src/util")

local GuiPopupMenu = {}

-- Args:
--  x: number?,
--  y: number?,
--  width: number?,
--  items: table array:
--			label: string,
--			action: func() callback,
--			divider: bool?,
--			stay_open: bool?
function GuiPopupMenu.new(parent, args)
	local popup_width = args.width or 100
	local popup_height = 10
	
	for item in all(args.items) do
		popup_height += item.divider and 8 or 12
	end
	
	local left = 4
	local right = 476 - popup_width
	local top = Util.os_toolbar_height + 4
	local bottom = 266 - popup_height
	
	local popup_x = Util.clamp(args.x or parent.sx, left, right)
	local popup_y = math.max(args.y or parent.sy + parent.height + 2, top)
	
	if popup_y > bottom then
		popup_y = Util.clamp(parent.sy - popup_height - 2, top, bottom)
	end
	
	local modal = parent.head:attach {
		x = 0,
		y = 0,
		width = 480,
		height = 270,
		click = function(widget)
			widget:detach()
		end
	}
	
	local pulldown = modal:attach_pulldown {
		x = popup_x,
		y = popup_y,
		width = popup_width,
		onclose = function() modal:click() end
	}
	
	for item in all(args.items) do
		pulldown:attach_pulldown_item {
			label = item.label,
			action = item.action,
			divider = item.divider,
			stay_open = item.stay_open
		}
	end
	
	return pulldown
end

return GuiPopupMenu

