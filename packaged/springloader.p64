	}
}

local _color_to_flavor = {
	"black", "black", "red", "green",		-- 0-3
	"orange", "gray", "gray", "gray",	-- 4-7
	"red", "orange", "orange", "green",	-- 8-11
	"blue", "purple", "pink", "orange",	-- 12-15
	"blue", "blue", "purple", "green",	-- 16-19
	"orange", "black", "gray", "pink",	-- 20-23
	"red", "orange", "green", "green",	-- 24-27
	"blue", "purple", "pink", "orange",	-- 28-31
}

local _color_table = get_spr(256)

local Colormap = {}

function Colormap.init()
	Colormap.apply_flavor("gray")
end

function Colormap.apply_flavor(flavor)
	local palette = fetch("pal/" .. flavor .. ".pal")
	
	if palette then palette:poke(0x5000) end
end

function Colormap.color_to_flavor(col)
	return _color_to_flavor[col + 1]
end

function Colormap.picotron_colors(flavor)
	return _pico_pal_colors[flavor] or _pico_pal_colors["gray"]
end

function Colormap.lowcolor_pal(os_theme)
	pal()
	
	-- Should find a way to do this without clobbering the palette
	for i = 1, 31 do
		pal(i, os_theme[_lowcolor_to_theme[_to_lowcolor[i]]])
	end
	
	-- Grab the resulting color table
	local pal_data = userdata("u8", 4096)
	pal_data:peek(0x8000)
	
	pal()
	
	return pal_data
end

function Colormap.draw_with_table(color_table, draw_func)
	memmap(color_table, 0x8000)
	poke(0x550b, 0x3f) -- Needed to make shape blending work
	draw_func()
	poke(0x550b, 0) -- To speed up drawing
	unmap(color_table)
end

function Colormap.draw_blended(draw_func)
	Colormap.draw_with_table(_color_table, draw_func)
end

return Colormap

