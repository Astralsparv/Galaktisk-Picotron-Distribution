		autoclose = false,
		can_escape_fullscreen = true
	}
	
	Util.show_toolbar()
	
	Screen:install_view(ViewAutoTiles)
	Screen:install_view(ViewBigTiles)
	Screen:install_view(ViewSmallTiles)
	Screen:install_view(ViewColumns)
	
	Screen:install_background(BGBasic)
	Screen:install_background(BGBubbles)
	Screen:install_background(BGPicotron)
	
	Screen:init()
end

function _update()
