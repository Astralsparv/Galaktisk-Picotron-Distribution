local Class = {}

function Class.class(class_obj)
	class_obj = class_obj or {}
	
	class_obj.new_obj = function(self, obj)
		obj = obj or {}
		
		self.__index = self
		return setmetatable(obj, self)
	end
	
	return class_obj
end

function Class.super(class_obj)
	local meta = getmetatable(class_obj) or {}
	
	return meta.__index
end

return Class

