--[[pod_format="raw",created="2025-12-07 18:00:00",modified="2025-12-13 20:11:28",revision=28,xstickers={}]]

local Util = require("src/util")

local GuiCategoryView = require("src/gui/category_view")

local _column_spacing = 4

local ViewColumns = {
	name = "Columns",
	config_name = "columns",
	max_categories = 3
}

-- Args:
--  x: number,
--  y: number,
--  width: number,
--  height: number,
--  categories: table,
--  active_categories: table,
--  theme: table
function ViewColumns.new(parent, args)
	local container = parent:attach {
		x = args.x,
		y = args.y,
		width = args.width,
		height = args.height
	}

	local category_x = 0
	local category_height = args.height
	local category_count = 0
	local columns = ViewColumns.max_categories
	local column_width = math.floor((args.width - ((columns - 1) * _column_spacing)) / columns)
	
	for category_name in all(args.active_categories) do
		local index = Util.index_by_key(args.categories, category_name, "name")
		
		if index > 0 then
			GuiCategoryView.new(container, {
				x = category_x,
				y = 0,
				width = column_width,
				height = category_height,
				big_icons = false,
				category = args.categories[index],
				theme = args.theme
			})
			
			category_x += column_width + _column_spacing
			category_count += 1
			
			if category_count >= columns then break end
		end
	end
	
	return container
end

return ViewColumns

