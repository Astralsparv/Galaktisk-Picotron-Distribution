--[[pod_format="raw",created="2025-12-10 23:04:01",modified="2025-12-11 21:00:20",revision=56,xstickers={}]]

local Pattern = require("src/pattern")
local Sfx = require("src/sfx")
local Util = require("src/util")

local GuiLaunchedItem = {}

function GuiLaunchedItem.new(parent, launcher)
	local capture = userdata(u8, launcher.width, launcher.height)
	
	set_draw_target(capture)
	camera()
	clip()
	
	launcher:draw({
		has_pointer = false,
		mb = 0
	})
	
	set_draw_target()
	
	Sfx.sfx_polite(5)
	
	return parent:attach {
		x = launcher.x + 1,
		y = launcher.y,
		width = launcher.width,
		height = launcher.height,
		ghost = true,
		capture = capture,
		center_x = launcher.icon_size / 2 + 2,
		timer = 0,
		launched_yet = false,
		path = launcher.item.path,
		update = function(widget)
			widget.timer += Util.time_delta * 4
			
			if widget.timer >= 1 and not widget.launched_yet then
				widget.launched_yet = true
				Util.os_launch(widget.path)
				
				widget:detach()
			end
		end,
		draw = function(widget)
			local lerp = math.sin(widget.timer * math.pi / 2)
			local width = widget.capture:width()
			local height = widget.capture:height()
			local scaled_width = width * (lerp + 1)
			local scaled_height = height * (lerp + 1)
			local offset_x = -widget.center_x * lerp
			local offset_y = (height - scaled_height) / 2
			
			clip()
			Pattern.set_screen_door(widget.timer)
			sspr(widget.capture, 0, 0, width, height, offset_x, offset_y, scaled_width, scaled_height)
			fillp()
		end
	}
end

return GuiLaunchedItem

