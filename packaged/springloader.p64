:: src/screen.lua
--[[pod_format="raw",created="2025-12-03 23:34:33",modified="2025-12-14 02:36:47",revision=1065,xstickers={}]]

local Category = require("src/category")
local Class = require("src/class")
local Colormap = require("src/colormap")
local Config = require("src/config")
local Draw = require("src/draw")
local Sfx = require("src/sfx")
local Util = require("src/util")

local GuiHeader = require("src/gui/header")
local GuiCategoryPane = require("src/gui/category_pane")

local _column_spacing = 4
local _pane_width = 80
local _body_y = 24 + Util.os_toolbar_height
local _footer_height = 13

local Screen = {
	plugins = {
		views = {},
		backgrounds = {},
	},
	gui = {},
	categories = {},
	active_categories = {},
	max_active_categories = 1,
	view = nil,
	view_class = nil,
	background = nil,
	theme = {},
	loaded = false,
	spring_timer = 0,
	offscreen_target = userdata("u8", 480, 270),
	config_dependencies = {}
}

function Screen:install_view(view_class)
	if not self.plugins.views[view_class.config_name] then
		add(self.plugins.views, view_class)
	
		self.plugins.views[view_class.config_name] = view_class
	end
end

function Screen:install_background(bg_class)
	if not self.plugins.backgrounds[bg_class.config_name] then
		add(self.plugins.backgrounds, bg_class)
		
		self.plugins.backgrounds[bg_class.config_name] = bg_class
	end
end

function Screen:init()
	fetch("font/p8_fixed.font"):poke(0x5600)
	
	Colormap.init()
	
	self.gui = create_gui()
	
	Config:load_current()
	self:refresh_theme()
	self:refresh_background()
	
	self:_load_last_categories()
	
	self:_update_view_class()
	
	on_event("gained_focus", function(msg)
		Util.show_toolbar()
		-- TODO: Only refresh apps on filesystem changes
		self:refresh_apps()
		self:boing()
	end)
	
	on_event("lost_focus", function(msg)
		self:_save_last_categories()
		self:_hide_ui()
	end)
	
	for option in all {
		"background", "color_theme", "picotron_colors", "color_blending",
		"reduced_motion", "sound"
	} do
		self.config_dependencies[option] = Screen.refresh_widgets
	end
end

function Screen:refresh_all()
	self:_update_view_class()
	self:refresh_apps()
end

function Screen:refresh_apps()
	self.categories = {}
	
	if Config.current.app_folder and Util.is_absolute_dir(Config.current.app_folder) then	
		Util.log("Refreshing apps in folder: " .. Config.current.app_folder)
		
		local root_category = Category.new("(base)", Config.current.app_folder, false)
		
		root_category:populate()
		
		if #root_category.items > 0 then
			add(self.categories, root_category)
		end
		
		local dir_list = ls(Config.current.app_folder)
		
		for dir_item in all(dir_list) do
			local full_path = Config.current.app_folder .. "/" .. dir_item
			
			if Util.os_is_folder(full_path) then
				local category = Category.new(dir_item, full_path, true)
				add(self.categories, category)
			end
		end
		
		local metadata = fetch_metadata(Config.current.app_folder)
		
		if metadata.springloader_preload then
			for category in all(self.categories) do
				category:populate()
			end
		end
		
		self:_adjust_active_categories()
	end
	
	self:refresh_widgets()
	
	self.loaded = true
end

function Screen:_adjust_active_categories()
	while #self.active_categories > self.max_active_categories do
		deli(self.active_categories)
	end
	
	for i = #self.active_categories, 1, -1 do
		if Util.index_by_key(self.categories, self.active_categories[i], "name") == 0 then
			deli(self.active_categories, i)
		end
	end
	
	if #self.active_categories < self.max_active_categories then
		for category in all(self.categories) do
			if Util.in_array(self.active_categories, category.name) == false then
				add(self.active_categories, category.name)
				
				if #self.active_categories >= self.max_active_categories then break end
			end
		end
	end
end

function Screen:_update_view_class()
	self.view_class = self.plugins.views[Config.current.view or ""]
	
	if self.view_class then
		self.max_active_categories = math.max(1, self.view_class.max_categories or 1)
	else
		self.max_active_categories = 1
	end
end

function Screen:_hide_ui()
	for widget in all(self.gui.head.child) do
		widget:detach()
	end
	
	local os_theme = fetch("/ram/shared/theme.pod") or {}
	
	self:refresh_theme(true)
end

function Screen:refresh_theme(force_pico_colors)
	local os_theme = fetch("/ram/shared/theme.pod") or {}
	local os_settings = fetch("/appdata/system/settings.pod") or {}
	local flavor_name = Config.current.color_theme or "os"
	
	if flavor_name == "os" then
		flavor_name = Colormap.color_to_flavor(os_theme.desktop0)
	end
	
	local colors = {}
	
	if force_pico_colors or Config.current.picotron_colors then
		local pico_colors = Colormap.picotron_colors(flavor_name)
		
		colors = {
			pico_colors.darker, pico_colors.dark, pico_colors.main,
			pico_colors.light, pico_colors.tint, pico_colors.input
		}
	else
		Colormap.apply_flavor(flavor_name)
		
		colors = { 33, 34, 35, 36, 37, 38 }
	end
	
	-- Reuse existing table so that existing widgets can pick up
	--  on theming changes
	Util.table_concat(self.theme, {
		back = colors[3],
		back1 = colors[4],
		back2 = colors[3],
		back_dark = colors[2],
		back_shadow = colors[1],
		text = 7,
		text_tint = colors[6],
		button = colors[1] + (colors[2] << 8),
		button_text = colors[5] + (7 << 8),
		input_bg = colors[6],
		input_fg = 1,
		input_select = colors[3],
		list = Config.current.color_blending and 61 or colors[2],
		list_border = Config.current.color_blending and 61 or colors[1],
		tile_hover = colors[1],
		label = colors[1],
		lowcolor_pal = Colormap.lowcolor_pal(os_theme),
		force_lowcolor = os_settings.lowcol_icons or false
	})
end

function Screen:refresh_background()
	local bg = self.plugins.backgrounds[Config.current.background]
	
	if bg and bg ~= Class.super(self.background) then
		self.background = bg.new()
	end
end

function Screen:refresh_widgets()
	self:refresh_theme()
	self:refresh_background()
	
	for widget in all(self.gui.head.child) do
		widget:detach()
	end
	
	GuiHeader.new(self.gui, {
		x = 0,
		y = Util.os_toolbar_height,
		current_config = Config.current,
		plugins = self.plugins,
		theme = self.theme,
		set_config_option = function(option, value)
			self:_set_config_option(option, value)
		end
	})
	
	if #self.categories <= 0 then
		return
	end
	
	local pane_x = get_display():width() - _pane_width - _column_spacing
	local pane_y = _body_y + 16
	
	GuiCategoryPane.new(self.gui, {
		x = pane_x,
		y = pane_y,
		width = _pane_width,
		height = get_display():height() - pane_y - _footer_height,
		categories = self.categories,
		active_categories = self.active_categories,
		theme = self.theme,
		activate_category = function(category_name)
			self:_activate_category(category_name)
		end
	})
	
	self:refresh_category_view()
end

function Screen:refresh_category_view()
	if self.view then
		self.view:detach()
		self.view = nil
	end
	
	if not self.view_class then return end
	
	local pane_x = get_display():width() - _pane_width - _column_spacing
	local view_height = get_display():height() - _body_y - _footer_height
	
	self.view = self.view_class.new(self.gui, {
		x = _column_spacing,
		y = _body_y,
		width = pane_x - _column_spacing * 2,
		height = view_height,
		categories = self.categories,
		active_categories = self.active_categories,
		theme = self.theme
	})
end

function Screen:_activate_category(category_name)
	Sfx.sfx_polite(2)

	del(self.active_categories, category_name)
	
	while #self.active_categories >= self.max_active_categories do
		deli(self.active_categories, 1)
	end
	
	add(self.active_categories, category_name)
	
	self:refresh_category_view()
end

function Screen:_set_config_option(option, value, skip_refresh)
	if Config.current[option] == nil or Config.current[option] == value then
		return false
	end
	
	Config.current[option] = value
	
	Config:save_current()
	
	local refresh_func = self.config_dependencies[option]
	
	if refresh_func ~= nil then
		refresh_func(self)
	else
		-- Not specified, assume we need to refresh everything just to be safe
		self:refresh_all()
	end
	
	return true
end

function Screen:_load_last_categories()
	for category in all(Config.current.last_categories) do
		if type(category) == "string" then
			add(self.active_categories, category)
		end
	end
end

function Screen:_save_last_categories()
	Config.current.last_categories = self.active_categories
	
	Config:save_current()
end

function Screen:boing()
	Sfx.sfx_polite(0)
	self.spring_timer = 0.5
end

function Screen:update()
	self.gui:update_all()
	
	if self.spring_timer > 0 then
		self.spring_timer = math.max(0, self.spring_timer - Util.time_delta)
	end
	
	if self.background and not Config.current.reduced_motion then
		self.background:update()
	end
end

function Screen:draw()
	local springing = self.spring_timer > 0.00001 and not Config.current.reduced_motion

	if springing then
		cls()
		set_draw_target(self.offscreen_target)
	end

	self:_draw_screen()
	
	set_draw_target()
	
	if springing then
		self:_draw_spring_screen()
	end
	
	if Util.dev_mode then
		print("\#0FPS: " .. string.format("%.1f", stat(7)),
			280, 260, 7)
		print("\#0CPU: " .. string.format("%.5f", stat(1)),
			340, 260, 7)
		print("\#0Mem: " .. tostring(math.ceil(stat(0) / 1024)) .. " KB",
			410, 260, 7)
	end
end

function Screen:_draw_screen()
	if self.background then
		self.background:draw(self.theme)
	else
		cls(self.theme.back)
	end

	self.gui:draw_all()
	
	if self.loaded and #self.categories <= 0 then
		Draw.label_multiline("Nothing found in " ..
			Config.current.app_folder .. "\n\nSure is empty in here...", {
				x = 240,
				y = 125,
				center_x = true,
				fg_col = self.theme.text_tint,
				bg_col = self.theme.list
			})
	end
end

function Screen:_draw_spring_screen()
	local oscillation = math.sin(self.spring_timer * math.pi * 16)
	local pos = oscillation * self.spring_timer * self.spring_timer * 32
	local width = self.offscreen_target:width()
	local height = self.offscreen_target:height()
	
	palt(0, false)
	sspr(self.offscreen_target, 0, 0, width, height,
		pos + 0.5, 0, width - pos * 2 + 0.5, height)
	palt()
end

return Screen

