--[[pod_format="raw",created="2025-12-08 01:52:18",modified="2025-12-12 22:55:00",revision=19,xstickers={}]]

local Text = {}

function Text.calculate_width(text, small_font)
	-- Calculation taken from OS gui.lua. TODO: Find better calculation
	return small_font and #text * 4 or #text * 5
end

function Text.calculate_width_multiline(lines, small_font)
	local width = 0
	
	for i = 1, #lines do
		width = max(width, Text.calculate_width(lines[i], small_font))
	end
	
	return width
end

function Text.split_lines(text, wrap_width, small_font)
	local lines = split(text, "\n", false)
	
	if not wrap_width or wrap_width <= 0 then return lines end
	
	local wrapped_lines = {}
	
	for line in all(lines) do
		local words = split(line, " ", false)
		local current_width = 0
		local wrapped = ""
		
		for word in all(words) do
			if #wrapped == 0 then
				wrapped = word
				current_width = Text.calculate_width(word, small_font)
			else
				local to_add = " " .. word
				local next_width = Text.calculate_width(to_add, small_font)
				if current_width + next_width > wrap_width then
					add(wrapped_lines, wrapped)
					wrapped = word
					current_width = Text.calculate_width(word, small_font)
				else
					wrapped = wrapped .. to_add
					current_width += next_width
				end
			end
		end
		
		if #wrapped > 0 then
			add(wrapped_lines, wrapped)
		end
	end
	
	return wrapped_lines
end

return Text

