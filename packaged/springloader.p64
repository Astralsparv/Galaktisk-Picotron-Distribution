local Class = require("src/class")
local Config = require("src/config")
local Util = require("src/util")

local _default_icons = {
	folder = 2,
	cart = 3,
	file = 4,
	loc = 5
}

local Category = Class.class()

function Category.new(name, path, include_folders)
	local category = Category:new_obj {
		name = name,
		path = path,
		include_folders = include_folders,
		items = {},
		populated = false
	}

	return category
end

function Category:populate()
	if self.populated then return end
	
	Util.log("Start populate: " .. stat(1))
	
	local dir_list = ls(self.path)
	
	Util.log("After ls: " .. stat(1))
	
	for dir_item in all(dir_list) do
		local full_path = self.path .. "/" .. dir_item
		local start_path = full_path
		local new_item_type = Util.os_item_type(full_path)
		
		if new_item_type == "loc" then
			full_path = Util.follow_loc_file(full_path)
			new_item_type = Util.os_item_type(full_path)
		end
		
		if self.include_folders or new_item_type ~= "folder" then
			local metadata = fetch_metadata(start_path)

			if metadata then
				local new_name = dir_item
				
				if metadata.title and #metadata.title > 0 then
					new_name = metadata.title
				end
				
				local default_icon = _default_icons[new_item_type] or 1
				
				local item = {
					name = new_name,
					icon = metadata.icon or default_icon,
					lowcolor = metadata.lowcol_icon or not metadata.icon,
					item_type = new_item_type,
					sort_key = tostring(default_icon) .. "_" .. string.lower(new_name),
					path = full_path
				}
				
				add(self.items, item)
			end
		end
	end
	
	Util.log("After read dir list: " .. stat(1))
	
	sort(self.items, "sort_key")
	
	Util.log("After sort: " .. stat(1))
	
	self.populated = true
end

return Category

