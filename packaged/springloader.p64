--[[pod_format="raw",created="2025-12-04 17:18:39",modified="2025-12-12 22:13:42",revision=199,xstickers={}]]

include "src/lib/sort.lua"

local Util = {
	dev_mode = (env().argv[0]:find("/system") == 1),
	checkmark = "\^:0020301a0e040000",
	os_toolbar_height = 11,
	time_delta = 1 / 60
}

if Util.dev_mode then
	include "src/lib/lens.lua"
	
	function Util.log(message)
		Lens.log(message)
	end
	
	Lens.log("Springloader using Lens for logging")
else
	function Util.log(message)
	end
end

function Util.clamp(value, low, high)
	return math.max(low, math.min(high, value))
end

function Util.in_array(array, item)
	for value in all(array) do
		if value == item then return true end
	end
	
	return false
end

function Util.index(array, item)
	for i, value in ipairs(array) do
		if value == item then return i end
	end
	
	return 0
end

function Util.index_by_key(array, item, key)
	for i, value in ipairs(array) do
		if value[key] == item then return i end
	end
	
	return 0
end

function Util.table_concat(dest, src)
	for k, v in pairs(src) do
		dest[k] = v
	end
	
	return dest
end

function Util.checkmark_text(text, enabled)
	return (enabled and Util.checkmark or "\-e  ") .. "\-f " .. text
end

function Util.is_absolute_dir(filepath)
	return #filepath > 0 and filepath[1] == "/"
end

function Util.is_web_player()
	-- Kinda hackish but oh well
	return #stat(150) > 0
end

function Util.os_item_type(filepath)
	local extension = filepath:ext()
	
	if extension and extension:is_cart() then
		return "cart"
	elseif extension == "loc" then
		return "loc"
	end
	
	local item_type = fstat(filepath)
	
	return item_type
end

function Util.os_is_folder(filepath)
	return Util.os_item_type(filepath) == "folder"
end

function Util.os_open_folder(folder_path)
	create_process("/system/apps/filenav.p64", {
		path = folder_path,
	})
end

function Util.os_about(filepath)

	create_process("/system/apps/about.p64", {
		argv = { filepath },
		window_attribs = { autoclose = true }
	})
end

local _item_type_to_launcher = {
	cart = function(filepath)
		create_process(filepath)
	end,
	folder = function(filepath)
		Util.os_open_folder(filepath)
	end,
	file = function(filepath)
		-- Code from filenav.p64
		create_process(env().open_with and env().open_with or "/system/util/open.lua", {
			argv = { filepath },
			fileview = {{ location = filepath, mode = "RW" }}, -- grant permission to access it (in case opening program is sandboxed)
		})
	end
}

function Util.os_launch(filepath)
	local item_type = Util.os_item_type(filepath)
	
	if item_type == "loc" then
		local new_path = Util.follow_loc_file(filepath)
		
		if new_path == filepath then
			notify("Invalid loc file at " .. filepath .. "!")
		elseif new_path then
			return Util.os_launch(new_path)
		end
	end
	
	local launcher = _item_type_to_launcher[item_type]
	
	if launcher then
		launcher(filepath)
	else
		notify("Unknown item type for " .. filepath .. "!")
	end
end

function Util.follow_loc_file(filepath)
	for i = 1, 32 do
		local data = fetch(filepath)
		
		if data and data.location and Util.is_absolute_dir(data.location) then
			filepath = data.location
			
			if Util.os_item_type(filepath) ~= "loc" then
				return filepath
			end
		else
			break
		end
	end
	
	-- If we got here, the .loc file was invalid,
	--  or the .loc chain is probably an infinite loop.
	--  Return the source path.
	
	return filepath
end

function Util.show_toolbar()
	send_message(3, {
		event = "dock_toolbar",
		state = true
	})
end

function Util.instance_running()
	if env().corun_program then
		return false -- Running as loaded dev cart, ignore
	end
	
	local proc_id = pid()
	local prog = env().argv[0]
	
	local procs = fetch("/ram/system/processes.pod")
	
	for proc in all(procs) do
		if proc.prog == prog and proc.id ~= proc_id then
			return true
		end
	end
	
	return false
end

return Util

