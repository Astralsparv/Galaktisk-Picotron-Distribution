--[[pod_format="raw",created="2025-12-09 22:13:50",modified="2025-12-13 01:54:35",revision=116,xstickers={}]]

local Colormap = require("src/colormap")
local Draw = require("src/draw")
local Sfx = require("src/sfx")
local Text = require("src/text")
local Util = require("src/util")

local GuiPopupMenu = require("src/gui/popup_menu")

local GuiLauncher = {}

-- Args:
--  x: number,
--  y: number,
--  width: number,
--  icon_size: number,
--  item: table,
--  theme: table,
--  launched: func(self) callback
function GuiLauncher.new(parent, args)
	local container = parent.parent
	
	return parent:attach {
		x = args.x or 0,
		y = args.y or 0,
		width = args.width or 100,
		height = args.icon_size + 4,
		item = args.item,
		icon_size = args.icon_size,
		theme = args.theme,
		cursor = "pointer",
		last_mb = 0,
		launched = args.launched,
		cache_label = function(widget)
			widget.cached = widget.cached or {}
			local cached = widget.cached
			
			local label_width = widget.width - widget.icon_size - 8
			cached.max_lines = widget.icon_size > 16 and 3 or 1
			
			cached.wrapped = Text.split_lines(widget.item.name, label_width, false)
			local width = Text.calculate_width_multiline(cached.wrapped)
			
			if #cached.wrapped > cached.max_lines or width > label_width then
				cached.max_lines += 1
				cached.wrapped = Text.split_lines(widget.item.name, label_width, true)
				cached.small_font = true
			else
				cached.small_font = false
			end
		end,
		draw_icon_raw = function(widget, x, y)
			sspr(widget.item.icon or 1, 0, 0, 16, 16, x, y, widget.icon_size, widget.icon_size)
		end,
		draw = function(widget, msg)
			if not widget.cached then
				widget:cache_label()
			end
			
			local big_icons = widget.icon_size > 16
			local offset = big_icons and 2 or 1
			local x_offset = msg.has_pointer and offset or 0
			local y_offset = (msg.mb > 0 and msg.has_pointer) and offset or 0
			local label_width = widget.width - widget.icon_size - 8
			
			if msg.has_pointer then
				Draw.border_rrect(0, 0, widget.width, widget.height, widget.theme.tile_hover)
			end
			
			if widget.item.lowcolor or widget.theme.force_lowcolor then
				Colormap.draw_with_table(widget.theme.lowcolor_pal, function() widget:draw_icon_raw(x_offset + 2, y_offset + 2) end)
			else
				widget:draw_icon_raw(x_offset + 2, y_offset + 2)
			end
			
			Draw.label_multiline(widget.cached.wrapped, {
				x = widget.icon_size + x_offset + 4,
				y = widget.height / 2 + y_offset,
				width = label_width,
				max_lines = widget.cached.max_lines,
				center_y = true,
				fg_col = widget.theme.text,
				bg_col = msg.has_pointer and widget.theme.tile_hover or widget.theme.label,
				small_font = widget.cached.small_font
			})
		end,
		click = function(widget, msg)
			widget.last_mb = msg.mb
			Sfx.sfx_polite(4)
		end,
		tap = function(widget, msg)
			if widget.last_mb == 1 then
				widget.launched(widget)
			elseif widget.last_mb == 2 then
				GuiLauncher._open_context_menu(widget)
			end
		end
	}
end

function GuiLauncher._open_context_menu(widget)
	local filename = widget.item.path:basename()
	
	local menu_items = {
		{
			label = "\^:ffff180418201800 Launch",
			action = function() widget.launched(widget) end
		},
		{
			divider = true
		},
		{
			label = "\^:0f013d2525243c00 Copy Path",
			action = function()
				set_clipboard(widget.item.path)
				notify("Copied to clipboard: " .. widget.item.path)
			end
		},
		{
			label = "\^:00387f7f7f7f7f00 Show in Filenav",
			action = function()
				local folder = widget.item.path:dirname()
				Util.os_open_folder(folder)
			end
		},
		{
			divider = true
		},
		{
			label = "About " .. filename,
			action = function()
				Util.os_about(widget.item.path)
			end
		},
	}
	
	GuiPopupMenu.new(widget, {
		width = 144,
		items = menu_items
	})
end

return GuiLauncher

