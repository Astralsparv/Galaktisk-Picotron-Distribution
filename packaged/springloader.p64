local Colormap = require("src/colormap")
local Text = require("src/text")

local Draw = {}

function Draw.blended_rrect(x, y, width, height, col)
	if col >= 60 or col == 0 then
		Colormap.draw_blended(function()
			rrect(x, y, width, height, 2, col)
		end)
	else
		rrect(x, y, width, height, 2, col)
	end
end

function Draw.blended_rrect_fill(x, y, width, height, col)
	if col >= 60 or col == 0 then
		Colormap.draw_blended(function()
			rrectfill(x, y, width, height, 2, col)
		end)
	else
		rrectfill(x, y, width, height, 2, col)
	end
end

function Draw.border_rrect(x, y, width, height, col, bord_col)
	if col ~= nil then
		Draw.blended_rrect_fill(x, y, width, height, col)
	end
	if bord_col ~= nil then
		Draw.blended_rrect(x, y, width, height, bord_col)
	end
end

-- Args:
--  x: number,
--  y: number,
--  width: number?,
--  height: number?,
--  center_x: bool?,
--  fg_col: number,
--  bg_col: number?,
--  bord_col: number?,
--  shad_col: number?,
--  small_font: bool?
function Draw.label(text, args)
	local x = args.x or 0
	local y = args.y or 0
	
	local label_text = text
	
	if args.small_font then
		label_text = "\014" .. text
	end
	
	if args.bg_col ~= nil or args.bord_col ~= nil then
		local text_width = Text.calculate_width(text, args.small_font)
		local width = args.width or text_width + 10
		local height = args.height or (args.small_font and 11 or 13)
		
		Draw.border_rrect(x, y, width, height, args.bg_col, args.bord_col)
		
		if args.center_x then
			x += math.max(0, (width - text_width - 10) / 2)
		end
		
		x += 4
		y += 3
	end
	
	if args.shad_col ~= nil then
		print(label_text, x, y - 1, args.shad_col)
	end
	print(label_text, x, y, args.fg_col)
end

-- Args:
--  x: number,
--  y: number,
--  width: number,
--  max_lines: number,
--  auto_wrap: bool?,
--  center_x: bool?,
--  center_y: bool?,
--  fg_col: number,
--  bg_col: number?,
--  bord_col: number?,
--  shad_color: number?,
--  small_font: bool?
function Draw.label_multiline(text, args)
	local start_x = args.x or 0
	local start_y = args.y or 0
	local line_height = args.small_font and 8 or 10
	local lines = text
	
	if type(lines) ~= "table" then
		lines = Text.split_lines(text, args.auto_wrap and args.width or 0, args.small_font)
	end
	
	local num_lines = min(#lines, args.max_lines or #lines)
	local box_width = Text.calculate_width_multiline(lines, args.small_font)
	local box_height = line_height * num_lines
	local has_rect = args.bg_col ~= nil or args.bord_col ~= nil
	
	if has_rect then
		start_x += 4
		start_y += 3
		box_width += 8
		box_height += 3
	end
	
	if args.center_x then
		start_x -= box_width / 2
	end
	if args.center_y then
		start_y -= box_height / 2
	end
	
	if has_rect then
		Draw.border_rrect(start_x - 4, start_y - 3, box_width, box_height, args.bg_col, args.bord_col)
	end
	
	for i = 1, num_lines do
		local line = lines[i]
		
		if i == num_lines and #lines > num_lines then
			line = sub(line, 0, -3) .. "..."
		end
		
		Draw.label(line, {
			x = start_x,
			y = start_y + (i - 1) * line_height,
			fg_col = args.fg_col,
			shad_color = args.shad_color,
			small_font = args.small_font
		})
	end
end

return Draw

