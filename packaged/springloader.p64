	}
	
	GuiCategoryView._reflow(scrollable)
	
	scroll_container:attach_scrollbars { autohide = true }
	
	return container
end

function GuiCategoryView._reflow(widget)
	widget.icon_size = widget.big_icons and 32 or 16
	widget.row_spacing = widget.big_icons and 4 or 1
	widget.row_height = widget.icon_size + 4 + widget.row_spacing

	widget.columns = widget.grid_layout and math.floor(widget.parent.width / _grid_column_min_width) or 1
	widget.item_width = math.floor((widget.parent.width - _scrollbar_width - ((widget.columns - 1) * _grid_column_spacing)) / widget.columns)
	widget.row_count = math.ceil(#widget.category.items / widget.columns)
	widget.height = widget.row_height * widget.row_count - widget.row_spacing
	
	widget._recycler = {
		y = 0,
		row_start = -1,
		row_end = -2,
		rows = {}
	}
	
	for launcher in all(widget.child) do
		launcher:detach()
	end
	
	GuiCategoryView._recycle(widget)
end

function GuiCategoryView._check_recycle(widget)
	if widget._recycler.y ~= widget.y then
		GuiCategoryView._recycle(widget)
	end
end

function GuiCategoryView._recycle(widget)
	widget._recycler.y = widget.y
	
	-- We need to get the diffs between the old and new row ranges.
	--  Then we need to remove any rows that scrolled out of view,
	--  and add any rows that scrolled in,
	--  without touching any unnecessary rows.
	
	local new_row_start = math.floor(-widget.y / widget.row_height)
	local new_row_end = new_row_start + math.ceil(widget.parent.height / widget.row_height)
	
	if new_row_start < widget._recycler.row_start then
		-- Start expanded on -y
		local add_end = math.min(widget._recycler.row_start - 1, new_row_end)
		
		GuiCategoryView._repopulate(widget, new_row_start, add_end)
	elseif new_row_start > widget._recycler.row_start then
		-- Start shrank on +y
		local remove_end = math.min(new_row_start - 1, widget._recycler.row_end)
		
		GuiCategoryView._remove_rows(widget, widget._recycler.row_start, remove_end)
	end
	
	if new_row_end > widget._recycler.row_end then
		-- End expanded on +y
		local add_start = math.max(widget._recycler.row_end + 1, new_row_start)
		
		GuiCategoryView._repopulate(widget, add_start, new_row_end)
	elseif new_row_end < widget._recycler.row_end then
		-- End shrank on -y
		local remove_start = math.max(new_row_end + 1, widget._recycler.row_start)
		
		GuiCategoryView._remove_rows(widget, remove_start, widget._recycler.row_end)
	end
	
	widget._recycler.row_start = new_row_start
	widget._recycler.row_end = new_row_end
end

function GuiCategoryView._remove_rows(widget, row_start, row_end)
	for i = row_start, row_end do
		local row_launchers = widget._recycler.rows[i]
		
		if row_launchers then
			for launcher in all(row_launchers) do
				launcher:detach()
			end
		else
			Util.log("Uh oh, tried to remove a row that doesn't exist! " .. i)
		end
		
		widget._recycler.rows[i] = nil
	end
end

function GuiCategoryView._repopulate(widget, row_start, row_end)
	-- Spawn in any new rows.
	
	for row = row_start, row_end do
		if widget._recycler.rows[row] then
			Util.log("Uh oh, tried to repopulate a row that already exists! " .. row)
			GuiCategoryView._remove_rows(widget, row, row)
		end
		
		widget._recycler.rows[row] = {}
	end
	
	row_start = row_start or 0
	row_end = row_end or widget.row_count - 1
	
	if row_end < row_start then return end
	
	local row = row_start
	local item_y = row_start * widget.row_height
	local column = 0
	local column_width = widget.item_width + _grid_column_spacing
	
	local item_start = row_start * widget.columns + 1
	local item_end = math.min((row_end + 1) * widget.columns, #widget.category.items)
	
	for i = item_start, item_end do
		local item = widget.category.items[i]
		local item_x = column_width * column
		
		local launcher = GuiCategoryView._make_launcher(widget, item_x, item_y, item)
		
		widget._recycler.rows[row] = widget._recycler.rows[row] or {}
		add(widget._recycler.rows[row], launcher)
		
		column += 1
		
		if column >= widget.columns then
			column = 0
			row += 1
			item_y += widget.row_height
		end
	end
end

function GuiCategoryView._make_launcher(widget, x, y, item)
	return GuiLauncher.new(widget, {
		x = x,
		y = y,
		width = widget.item_width,
		icon_size = widget.icon_size,
		item = item,
		theme = widget.theme,
		launched = function(launcher)
			if widget.launch_cooldown <= 0 then
				widget.launch_cooldown = 2
				GuiLaunchedItem.new(widget, launcher)
			end
		end
	})
end

return GuiCategoryView

