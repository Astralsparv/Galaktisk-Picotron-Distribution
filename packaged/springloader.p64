local Colormap = require("src/colormap")
local Draw = require("src/draw")
local Util = require("src/util")

local GuiButton = require("src/gui/button")

local GuiCategoryPane = {}

-- Args:
--  x: number,
--  y: number,
--  width: number,
--  height: number,
--  categories: table,
--  active_categories: table,
--  theme: table,
--  activate_category: func(string) callback
function GuiCategoryPane.new(parent, args)
	local pane = parent:attach {
		x = args.x,
		y = args.y,
		width = args.width,
		height = args.height,
		user_args = args
	}
	
	local scroll_container = pane:attach {
		x = 0,
		y = 0,
		width = pane.width,
		height = pane.height
	}
	
	local scrollable = scroll_container:attach {
		x = 0,
		y = 0,
		width = scroll_container.width,
		height = 18 * #args.categories
	}
	
	local need_scroll = scrollable.height > scroll_container.height
	local category_width = need_scroll and scrollable.width - 10 or scrollable.width
	local category_y = 0
	
	for category in all(args.categories) do
		local button = GuiButton.new(scrollable, {
			x = 0,
			y = category_y,
			width = category_width,
			label = category.name,
			theme = args.theme,
			update = function(widget)
				-- GUI processes every item even if it's out of view,
				--  so we need to optimize where we can
				-- TODO: virtualize items so only those in view are in the GUI tree
				widget.hidden = scrollable.y + widget.height < -widget.y or scrollable.y - scroll_container.height > -widget.y
				
				if not widget.hidden then
					widget.checked = Util.in_array(args.active_categories, category.name)
				end
			end,
			tap = function(widget)
				args.activate_category(category.name)
			end
		})
		
		category_y += 18
	end
	
	if need_scroll then
		scroll_container:attach_scrollbars()
	end
	
	return pane
end

return GuiCategoryPane

