--[[pod_format="raw",created="2025-12-11 20:57:20",modified="2025-12-11 20:57:31",revision=1,xstickers={}]]

local Config = require("src/config")

local Sfx = {}

function Sfx.sfx_polite(sound)
	if not Config.current.sound then return end
	
	-- Check if another process is playing audio and only play if
	--  it wouldn't interrupt.
	local active_proc = stat(499)
	local active_channels = stat(498)
	
	-- Also ignore system process so OS startup doesn't block sound
	-- TODO: Is it too risky to just check that no channels are active? What if a background app still wants control?
	if active_proc <= 1 or active_proc == pid() or active_channels == 0 then
		sfx(sound)
	end
end

return Sfx

