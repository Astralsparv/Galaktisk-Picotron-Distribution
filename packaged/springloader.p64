local Draw = require("src/draw")
local Sfx = require("src/sfx")
local Text = require("src/text")

local GuiButton = {}

-- Args:
--  x: number,
--  y: number,
--  label: string,
--  width: number?,
--  center_x: bool?
--  cursor: number/string?
--  theme: table,
--  update: func callback,
--  tap: func callback,
function GuiButton.new(parent, args)
	local text = args.label or "???"
	local width = args.width or Text.calculate_width(text) + 10
	local center_x = args.center_x
	
	if center_x == nil then
		center_x = true
	end

	return parent:attach {
		x = args.x or 0,
		y = args.y or 0,
		label = args.label,
		width = width,
		height = 14,
		center_x = center_x,
		is_active = args.is_active,
		cursor = args.cursor or "pointer",
		theme = args.theme,
		click = function(widget)
			Sfx.sfx_polite(4)
		end,
		tap = args.tap,
		update = args.update,
		draw = function(widget, msg)
			local y_offset = (msg.mb > 0 and msg.has_pointer) and 1 or 0
			local bg_col = widget.theme.button
			local fg_col = widget.theme.button_text
			local bord_col = nil
			
			if msg.has_pointer or widget.checked then
				bg_col >>= 8
				fg_col >>= 8
				bord_col = widget.theme.button
			end
			
			Draw.label(widget.label, {
				x = 0,
				y = y_offset,
				width = widget.width,
				height = widget.height - 1,
				center_x = widget.center_x,
				fg_col = fg_col,
				bg_col = bg_col,
				bord_col = bord_col
			})
		end
	}
end

return GuiButton

